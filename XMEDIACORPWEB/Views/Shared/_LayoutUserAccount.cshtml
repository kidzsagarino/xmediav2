@model BusinessModel.ObjectModel.LoginReturnInformationObjectModel
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_LayoutUserAccount</title>
    <link href="~/css/NewUserAccount.css" rel="stylesheet" />
    <link href="~/css/AboutUs.css" rel="stylesheet" />
    <link href="~/css/LayoutUserAccount.css" rel="stylesheet" />
    <link href="~/css/UserDashboard.css" rel="stylesheet" />
    <link href="~/css/UserLoginAccount.css" rel="stylesheet" />
</head>
<body id="customerLayoutBody">
    <header id="customerLayoutHeader">
        <div id="customerLayoutHeaderWrapper1">
            <div id="customerLayoutHeaderWrapper2_logo">
                <img src="~/images/x-media_logo.png" />
            </div>
            <div id="customerLayoutHeaderWrapper2_search">
                <div id="customerLayoutHeaderWrapper3_search">
                    <div>
                        <img src="~/images/icon/search.png" />
                    </div>
                    <input placeholder="Search" />

                </div>
            </div>

            <div id="customerLayoutHeaderWrapper2_miscMenu">

                <div class="loginNameAndCircleIconDiv0">
                    <div class="loginNameAndCircleIconDiv1">
                        <h2 id="userLayoutUserNameDisplay">@Model.DisplayName</h2>
                        <h2 id="userLayoutUserNameIcon">@Model.NameInitial</h2>
                    </div>

                </div>
            </div>

            <div id="customerLayoutHeaderWrapper2_logout">
                <div class="userLayoutRigthMenuIconDiv">
                    <div class="menuIConDivContainer">
                        <div class="menuIConDiv"></div>
                        <div class="menuIConDiv"></div>
                        <div class="menuIConDiv"></div>
                    </div>
                    <div class="menuMenuDivContainer">
                        <div >
                            <img src="~/images/icon/logout30pxX30px.png" />
                            <h4>Log-out</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <content id="customerLayoutContent">
        @RenderBody()
    </content>
    <footer id="customerLayoutFooter"></footer>
    <script src="~/Scripts/UserDashboard.js"></script>
</body>
</html>

